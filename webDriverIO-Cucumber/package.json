    "webdriverio": "^6.5.2"
  },
  "dependencies": {
    "@types/cucumber": "^6.0.1",
    "@types/jquery": "^3.5.2",
    "allure-commandline": "^2.13.0",
    "class-transformer": "^0.3.1",
    "jsonfile": "^6.0.1",
    "wdio-video-reporter": "^3.1.1"
  }
}
