{
  "name": "webDriverIO-Cucumber",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:all": "npx wdio run wdio.conf.js",
    "test:base": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@bank_base_features'",
    "test:citibank": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@citibank_feature'",
    "test:axis": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@axis_bank_feature'",
    "test:smoke": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@smoke'",
    "test:regression": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@regression'",
    "test:sanity": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@sanity'",
    "test:baseORciti": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@bank_base_features or @citibank_feature'",
    "test:baseORaxis": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@bank_base_features or @axis_bank_feature'",
    "test:smokeORregression": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@smoke or @regression'",
    "test:smokeORsanity": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@smoke or @sanity'",
    "test:sanityORregression": "npx wdio run wdio.conf.js --cucumberOpts.tagExpression='@sanity or @regression'"
  },
